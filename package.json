{
    "name": "jest_workaround",
    "version": "0.1.14",
    "description": "[SWC plugin] workaround for jest",
    "author": "magic-akari <akari.ccino@gmail.com>",
    "license": "MIT",
    "keywords": [
        "swc-plugin",
        "jest"
    ],
    "scripts": {
        "test:debug": "NODE_ENV=development pnpm build:debug && jest",
        "build:debug": "cargo build --target wasm32-unknown-unknown && cp -rf target/wasm32-unknown-unknown/debug/jest_workaround.wasm ./debug",
        "test": "pnpm build && jest",
        "build": "cargo prepublish --release && cp -rf target/wasm32-unknown-unknown/release/jest_workaround.wasm ./lib",
        "prepack": "pnpm build"
    },
    "packageManager": "pnpm@7.29.3",
    "main": "lib/jest_workaround.wasm",
    "files": [
        "src",
        "lib"
    ],
    "homepage": "https://github.com/magic-akari/jest_workaround#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/magic-akari/jest_workaround.git"
    },
    "bugs": {
        "url": "https://github.com/magic-akari/jest_workaround/issues"
    },
    "devDependencies": {
        "@swc/core": "^1.3.40",
        "@swc/jest": "^0.2.24",
        "@types/jest": "^29.4.4",
        "jest": "^29.5.0"
    },
    "peerDependencies": {
        "@swc/core": "^1.3.40",
        "@swc/jest": "^0.2.22"
    }
}